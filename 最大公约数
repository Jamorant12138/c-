#include <stdio.h>

int main()
{
	int a = 0;
	int b = 0;
	int i = 1;
	int divisor = 0;
	scanf("%d%d", &a, &b);
	while (i <= a && i <= b)
	{
		if (0 == a % i)
		{
			if (0 == b % i)
			{
				divisor = i;
				i++;
			}
			else
			{
				i++;
			}
		}
		else
		{
			i++;
		}
	}
	printf("%d\n", divisor);
	
	return 0;
}
