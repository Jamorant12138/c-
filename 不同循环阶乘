#include <stdio.h>

int main()
{
	int i = 2;
	int project = 1;
	int n = 0;
	scanf("%d", &n);
	do {
		project = project * i;
		i++;
	} while (i < n + 1);
	printf("%d\n", project);

	return 0;
}

