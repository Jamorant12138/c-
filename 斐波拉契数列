int main()
{
	int x = 1;
	int y = 1;
	printf("%d %d ", x, y);   
	while (x <= 100)       //利用减法还原x的旧值
	{
		x = x + y;
		y = x - y;
		printf("%d ", x);
	}
	return 0;
}
